-bool statusCriteria::checkState(stringList& reasons)
-{
-  if(type == STATUS)
-  {
-    stringList internalMatches;
-    stringList internalMismatches;
-
-    bool fMatch = true;
-
-    if(statusOp == OP_OR)
-      fMatch = false;
-
-    for(statusList_::iterator i = statusList.begin(); i != statusList.end(); ++i)
-    {
-      int pos = i->getStatus().find(".");
-      if(pos != std::string::npos)
-      {
-        std::string compName = i->getStatus().substr(0, pos);
-        std::string compState = i->getStatus().substr(pos + 1);
-        std::string realCompState;
-
-        bool match = false;
-        component::list::iterator comp = components.find(compName);
-        if(comp != components.end())
-        {
-          realCompState = comp->second->getState();
-          if(comp->second->checkState(compState))
-            match = true;
-        }
-
-        std::string temp(compName);
-        temp.append(".");
