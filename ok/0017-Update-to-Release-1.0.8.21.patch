-
-#else
-#define LOCK()
-#define UNLOCK()
-
-#endif
-
-
-
-class _aMap;
-
-struct oBuf
-{
-  long  lSize;
-  char *pData;
-  oBuf( const char *s, long len )
-  {
-    if(len)
-    {
-      lSize = len;
-      pData = new char[lSize+1];
-      memcpy(pData, s, len);
-      pData[len] = 0;     // trailing zero!!
