-
-          case OP_AND:
-            fMatch = false;
-            internalMismatches.push_back(temp);
-            break;
-
-          case OP_EQUALS:
-            reasons.push_back(temp);
-            return false;
-          }
-        }
-      }
-    }
-
-    if(fMatch)
-      reasons.insert(reasons.end(), internalMatches.begin(), internalMatches.end());
-    else
-      reasons.insert(reasons.end(), internalMismatches.begin(), internalMismatches.end());
-    return fMatch;
-  }
