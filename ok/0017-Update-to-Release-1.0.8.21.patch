-  };
-
-  pair    *sTab;
-  short    limit,       // table size
-           used,
-           fence,
-           bgrow;
-
-  _aMap( const long n )
-  {
-    fence = (short)n;
-    if( fence < 3 ) fence = 3;
-    used = bgrow = 0;
-    limit =(short)((fence * 10)/7);
-    sTab = new pair[limit];
-  }
-
-  _aMap( const _aMap& src )
-  {
-    fence = src.fence; bgrow = 0;
-    used  = src.used;
-    limit = src.limit;
-    sTab = new pair[limit];
-    for(long i=0; i<limit; i++)
-    { // copy contents
-      sTab[i].key = src.sTab[i].key;
-      sTab[i].val = src.sTab[i].val;
-    }
-  }
-
-  ~_aMap() { delete [] sTab; }
-
-};
