-  static unsigned int rootOID[] = { 1, 3, 6, 1, 4, 1, 192, 6, 4, 3 };
-  static int rootIDLength = 10;
-
-  class oID
-  {
-    unsigned int* pIDs;
-    int length;
-
-    void copy(unsigned int* pIDs, int length)
-    {
-      if(pIDs == 0 || length == 0)
-      {
-        this->pIDs = 0;
-        this->length = 0;
-        return;
-      }
-
-      this->pIDs = new unsigned int[length];
-      for(int i = 0; i < length; ++i)
-        this->pIDs[i] = pIDs[i];
-      this->length = length;
-    }
-
-  public:
-    oID() : pIDs(0), length(0)
-    {
-    }
-
-    oID(unsigned int id)
-    {
-      length = 1;
-      pIDs = new unsigned int[1];
-      pIDs[0] = id;
-    }
-
-    oID(unsigned int* pIDs, int length)
-    {
-      copy(pIDs, length);
-    }
-
-    oID(const oID& oid)
-    {
-      copy(oid.getIDs(), oid.getLength());
-    }
-
-    unsigned int* getIDs() const
-    {
-      return pIDs;
-    }
-
-    int getLength() const
-    {
-      return length;
-    }
-
-    oID& append(unsigned int id)
-    {
-      return *this + oID(id);
-    }
-
-    oID& operator + (const oID& add)
-    {
-      oID temp(*this);
-      if(pIDs)
-        delete[] pIDs;
-
-      length = temp.getLength() + add.getLength();
