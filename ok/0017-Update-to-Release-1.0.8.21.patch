-
-// Extension Agent DLLs provide the following entry point to communcate traps
-// to the Extendible Agent.  The Extendible Agent will query this entry point
-// when the trap Event (supplied at initialization time) is asserted, which
-// indicates that zero or more traps may have occured.  The Extendible Agent
-// will repetedly call this entry point until FALSE is returned, indicating
-// that all outstanding traps have been processed.
-
-BOOL WINAPI SnmpExtensionTrap(OUT AsnObjectIdentifier *enterprise,
-                              OUT AsnInteger          *genericTrap,
-                              OUT AsnInteger          *specificTrap,
-                              OUT AsnTimeticks        *timeStamp,
-                              OUT SnmpVarBindList  *variableBindings)
-{
-  bool fTrapSent = false;
-
-  writeTrace(BasicRuntime_HERE, "SnmpExtensionTrap--------------------------------------------------");
-
-  mtxQ.request();
-
-  try
-  {
-    if(trapQ.size() > 0)
-    {
-      enterprise->idLength = massaiMIB::getOIDLength("massai.notification");  // ".nCussEvent"
-      enterprise->ids = massaiMIB::getOID("massai.notification");             // ".nCussEvent"
-
-      std::string oidString;
-
-      massaiMIB::getOIDString("massai.notification.nCUSSEvent", oidString);
-      writeTrace(BasicRuntime_HERE, "massai.notification.nCUSSEvent = %s", oidString.c_str());
-
-      trapEvent* pTe = trapQ.front();
-      trapQ.pop();
-
-      writeTrace(BasicRuntime_HERE, "SnmpExtensionTrap([%s]), id = %d", pTe->getTrapText().c_str(),pTe->getID());
-
-      *genericTrap      = SNMP_GENERICTRAP_ENTERSPECIFIC;
-      *specificTrap     = pTe->getID();                    // 13.03.2003 (AGe)
-
-      *timeStamp        = GetCurrentTime() - dwTimeZero;
-
-      int iVariables = 2;
-      RFC1157VarBind* pVB = (RFC1157VarBind*)SnmpUtilMemAlloc(iVariables * sizeof(RFC1157VarBind));
-      memset(pVB, 0, iVariables * sizeof(RFC1157VarBind));
-
-      variableBindings->list = pVB;
-      variableBindings->len  = iVariables;
-
-      massaiMIB::getOIDString("massai.notification.nLastMessageID", oidString);
-      writeTrace(BasicRuntime_HERE, "massai.notification.nLastMessageID = %s", oidString.c_str());
-
-      AsnObjectIdentifier oID;
-      oID.ids = massaiMIB::getOID("massai.notification.nLastMessageID");
-      oID.idLength = massaiMIB::getOIDLength("massai.notification.nLastMessageID");
-
-      SnmpUtilOidCpy(&pVB->name, &oID);
-      pVB->value.asnType = ASN_GAUGE32;
-      pVB->value.asnValue.unsigned32 = pTe->getID();
-
-      ++pVB;
-
-      massaiMIB::getOIDString("massai.notification.nLastMessageText", oidString);
-      writeTrace(BasicRuntime_HERE, "massai.notification.nLastMessageText = %s", oidString.c_str());
-
-      oID.ids = massaiMIB::getOID("massai.notification.nLastMessageText");
-      oID.idLength = massaiMIB::getOIDLength("massai.notification.nLastMessageText");
-
-      SnmpUtilOidCpy(&pVB->name, &oID);
-      pVB->value.asnType = ASN_OCTETSTRING;
-      pVB->value.asnValue.string.dynamic = 0;
-      long lSize = pTe->getDescription().size() + 1;
-      pVB->value.asnValue.string.stream = (unsigned char *)SnmpUtilMemAlloc(lSize);
-      memcpy(pVB->value.asnValue.string.stream, pTe->getDescription().c_str(), lSize);
-      pVB->value.asnValue.string.length = lSize;
-
-      variableBindings->len = iVariables;
-
-      delete pTe;
-
-      // Indicate that valid trap data exists in the parameters.
-      fTrapSent = true;
-    } else
-    {
-      writeTrace(BasicRuntime_HERE, "SnmpExtensionTrap([])");
-    }
-  }
-  catch(...)
-  {
-      writeTrace(BasicRuntime_HERE, "Exception in SnmpExtensionTrap([])");
-  }
-  mtxQ.release();
-  return fTrapSent;
-}
-
-// Extension Agent DLLs provide the following entry point to resolve queries
-// for MIB variables in their supported MIB view (supplied at initialization
-// time).  The requestType is Get/GetNext/Set.
-
-BOOL WINAPI SnmpExtensionQuery(IN BYTE                   requestType,
-                               IN OUT RFC1157VarBindList *variableBindings,
-                               OUT AsnInteger            *errorStatus,
-                               OUT AsnInteger            *errorIndex)
-{
-  *errorStatus = SNMP_ERRORSTATUS_NOERROR;
-  *errorIndex = 0;
-  return SNMPAPI_NOERROR;
-}
-
-VOID WINAPI SnmpExtensionClose()
-{
-  static int iCloseCalls = 0;
-
-  writeTrace(BasicRuntime_HERE, "SnmpExtensionClose() call number %d", ++iCloseCalls);
-
-  if(iCloseCalls == 1)
-  {
-    if(pTrapListener)
-    {
-      delete pTrapListener;
-      pTrapListener = 0;
-    }
-
-    CloseHandle(hEventTrap);
-    configSNMP::cleanUp();
-    mLog::stopService();
-  }
-}
