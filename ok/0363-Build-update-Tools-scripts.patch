+++ b/SelfServiceBuild/Build/release/mk_Platform_NightlyBuild_4.33.00_Avinor.cmd
@@ -0,0 +1,40 @@
+call c:\Subversion\startup.cmd
+
+set USE_PRIVATE_ENV=1
+
+set PATH=%PATH%;c:\Tools
+
+set VC_COMPILER_VERSION=VC10
+
+if not (%VC_COMPILER_VERSION%)==(VC10) goto USE_VC6__
+call "c:\Program Files (x86)\Microsoft Visual Studio 10.0\VC\Bin\VCVARS32.BAT"
+goto END_USE_VC6__
+:USE_VC6__
+call "c:\Program Files (x86)\Microsoft Visual Studio\VC98\Bin\VCVARS32.BAT"
+:END_USE_VC6__
+
+set JAVA_HOME=c:\Program Files (x86)\java\jdk1.7.0_21
+
+if not (%M2_HOME%)==() goto M2_HOME_SET
+set M2_HOME=C:\Maven
+:M2_HOME_SET
+
+@echo on
+set BUILD_VERSION=4.33.00_Avinor
+set B_NAME=Platform Nightly Build 4.33.00_Avinor
+set LOGFILE=NightlyBuild_Platform-4.33.00_Avinor.log
+set BUILD_STREAM=SelfServicePlatform
+set LOCALSTORE=L:\Platform\NightlyBuild-4.33.00_Avinor
+
+set RECIPIENTS=Thomas.Meinke@materna.de,klaus.kluge@materna.de,thomas.geisler@materna.de,thomas.stegemann@materna.de
+
+set SVN_PROJECT=SelfServicePlatform
+set SVN_TAG=branches/RC_4.33.00_Avinor
+
+del /q %tmp%\properties.txt
+
+%~d0
+cd %~p0
+echo Start at %DATE%-%TIME% >%LOGFILE% 
+nmake -f makefile-svn nb_Platform_full %* >>%LOGFILE%
+echo Finished at %DATE%-%TIME% >>%LOGFILE%
