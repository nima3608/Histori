diff --git a/SelfServiceBuild/Build/release/makefile-svn_20120423 b/SelfServiceBuild/Build/release/makefile-svn_20120423
new file mode 100644
index 0000000..de047ab
--- /dev/null
+++ b/SelfServiceBuild/Build/release/makefile-svn_20120423
@@ -0,0 +1,139 @@
+################################################################################
+##
+##  PROJECT:        MASSAI
+##
+##  FILE:           all.mak
+##
+##  DATE:           05.07.2005
+##
+##  EDITOR:         Materna Information & Communications (PtM)
+##
+################################################################################
+
+SELF = makefile
+
+!if "$(BUILD_PATH)_" == "_"
+BUILD_PATH=L:\
+!endif
+
+!if "$(SVN_ROOT)_" == "_"
+SVN_ROOT=http://subversion.materna.de:7770/svn-repos
+!endif
+
+!if "$(DISTRIBUTE_PATH)_" == "_"
+DISTRIBUTE_PATH=d:\BuildOutput
+!endif
+
+!if "$(LOCALSTORE)_" == "_"
+LOCALSTORE=$(BUILD_PATH)\$(SVN_PROJECT)\$(SVN_TAG)
+!endif
+
+!if "$(RECIPIENTS)_" == "_"
+RECIPIENTS=michael.patzer@materna.de
+!endif
+
+!if "$(B_NAME)_" == "_"
+B_NAME="Nightly Build"
+!endif
+
+DISTRIBUTETARGET=$(DISTRIBUTE_PATH)\$(SVN_PROJECT)\$(SVN_TAG)
+REPO_PATH=$(SVN_ROOT)/$(SVN_PROJECT)/$(SVN_TAG)
+
+
+
+ErrorLog=result.log
+
+#===================================================================
+#
+#  Targets
+#
+#===================================================================
+
+all:           init
+
+
+init:          $(SELF)
+               @echo MAKEDIR          = $(MAKEDIR)
+               @echo LOGFILE          = $(LOGFILE)
+               @echo LOCALSTORE       = $(LOCALSTORE)
+               @echo DISTRIBUTETARGET = $(DISTRIBUTETARGET)
+               @echo REPO_PATH        = $(REPO_PATH)
+
+clean:         $(SELF)
+               IF EXIST $(LOCALSTORE) ( rd $(LOCALSTORE) /s /q ) ELSE ( echo nothing to remove )
+                                                                    
+checkout:      $(SELF)
+               cmd.exe /c svn co $(REPO_PATH) $(LOCALSTORE) --ignore-externals
+
+set_props:     $(SELF)
+               IF EXIST $(PROPERTIES_FILE) ( svn.exe propset svn:externals -F $(PROPERTIES_FILE) $(LOCALSTORE) )
+
+update:        $(SELF)
+               svn up $(LOCALSTORE)
+
+build_java:    $(SELF)
+               - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\Build\cpp && \
+               setenv.bat && \
+               xmk java
+
+build_cpp:     $(SELF)
+               - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\Build\cpp && \
+               setenv.bat && \
+               xmk /I
+
+copy:          $(SELF)
+               cmd.exe /c $(MAKEDIR)\distributeFiles.cmd $(LOCALSTORE) $(DISTRIBUTETARGET)
+
+sendMail:      $(SELF)
+               - cd /D $(MAKEDIR) && checkForErrors.cmd $(LOGFILE) $(ErrorLog) && \
+               $(MAKEDIR)\blat.exe \
+               -bodyF $(ErrorLog) \
+               -s "$(B_NAME): $(REPO_PATH)" \
+               -to $(RECIPIENTS) \
+               -f nobody@materna.de \
+               -server mail.materna.de \
+               -attacht $(LOGFILE)
+
+sendMailZip:   $(SELF)
+               - cd /D $(MAKEDIR) && checkForErrors.cmd $(LOGFILE) $(ErrorLog) && \
+               zip -u $(LOGFILE).zip $(LOGFILE) && \
+               $(MAKEDIR)\blat.exe \
+               -bodyF $(ErrorLog) \
+               -s "$(B_NAME): $(REPO_PATH)" \
+               -to $(RECIPIENTS) \
+               -f nobody@materna.de \
+               -server mail.materna.de \
+               -attach $(LOGFILE).zip
+
+#===================================================================
+#   Projects
+#===================================================================
+
+create:             init checkout update
+
+build_application:  create build_cpp
+
+nb_Platform:        clean init checkout set_props update build_cpp sendMailZip
+
+build_mavenQBG:     $(SELF)
+                    - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\Massai\java && \
+                    mv.cmd && cc.cmd && \
+                    mvn -fn install
+                    - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\Massai\java\adminApp && \
+                    mv.cmd && cc.cmd && \
+                    mvn -fn install
+                    - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\Massai\flex && \
+                    mv.cmd && cc.cmd && \
+                    mvn -fn install
+zip_QBG:            $(SELF)             
+                    cd /d $(LOCALSTORE)\$(BUILD_STREAM)\Build && \
+                    $(LOCALSTORE)\$(BUILD_STREAM)\Build\cpp\setenv.bat && \
+                    xmk zip /I
+QBG:                clean create build_cpp build_mavenQBG zip_QBG
+QBGMail:            clean create build_cpp build_mavenQBG zip_QBG sendMailZip
+nb_QBG:             QBG copy sendMailZip
+fast_QBG:           init update build_cpp build_mavenQBG zip_QBG copy sendMailZip
+
+#===================================================================
+#  End of File
+#===================================================================
