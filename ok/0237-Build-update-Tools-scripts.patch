                setenv.bat && \
                xmk installer
+
+build_driver:    $(SELF)
+               - cd /D $(LOCALSTORE)\$(BUILD_STREAM)\$(BUILD_WORKDIR) && \
+               setenv.bat && \
+               xmk driver
                
