@@ -0,0 +1,63 @@
+@echo off
+echo Build overview: > %2
+
+fgrep -n -B 10 -A 5 -s "fatal" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "fatal" errors >> %2
+) else (
+echo ################################### >> %2
+echo "fatal: " >> %2
+fgrep -n -B 10 -A 5 "fatal" %1 >> %2
+fgrep -A 2 "# Fehler   :" %1 >> %2
+)
+
+fgrep -n -B 10 -A 5 "BUILD FAILURE" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "BUILD FAILURE" errors >> %2
+) else (
+echo ################################### >> %2
+echo "BUILD FAILURE: " >> %2
+fgrep -n -B 10 -A 5 "BUILD FAILURE" %1 >> %2
+)
+
+fgrep -n -B 10 -A 5 "BUILD ERROR" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "BUILD ERROR" errors >> %2
+) else (
+echo ################################### >> %2
+echo "BUILD ERROR: " >> %2
+fgrep -n -B 10 -A 5 "BUILD ERROR" %1 >> %2
+)
+
+fgrep -n -B 10 -A 5 -s "FAILED [" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "FAILED" errors >> %2
+) else (
+echo ################################### >> %2
+echo "FAILED: " >> %2
+fgrep -n -B 10 -A 5 "FAILED [" %1 >> %2
+)
+
+fgrep -n -B 10 -A 5 "cannot find symbol" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "cannot find symbol" errors >> %2
+) else (
+echo ################################### >> %2
+echo "cannot find symbol: " >> %2
+fgrep -n -B 10 -A 5 "cannot find symbol" %1 >> %2
+)
+
+fgrep -n -B 10 -A 5 "[ERROR]" %1 > temp.log
+if %ERRORLEVEL%==1 (
+echo No "[ERROR]" errors >> %2
+) else (
+echo ################################### >> %2
+echo "[ERROR]: " >> %2
+fgrep -n -B 10 -A 5 "[ERROR]" %1 >> %2
+)
+
+
+
+
+del temp.log
+ 
