+
+FOR /F " tokens=1,2,3,4,5,6* delims=,:.- " %%i in ("%TIMESTAMP%") do set TIMESTAMP=%%k%%j%%i%%l%%m%%n
+@echo %TIMESTAMP%
+
+if not exist %2\%TIMESTAMP% md %2\%TIMESTAMP%
+if not exist %2\%TIMESTAMP%\bin md %2\%TIMESTAMP%\bin
+if not exist %2\%TIMESTAMP%\dll md %2\%TIMESTAMP%\dll
+if not exist %2\%TIMESTAMP%\jar md %2\%TIMESTAMP%\jar
+
+copy m:\%1\SelfServiceCUSS\Build\bin\*.* %2\%TIMESTAMP%\bin\*.*
+copy m:\%1\SelfServiceCUSS\Build\dll\*.* %2\%TIMESTAMP%\dll\*.*
+copy m:\%1\SelfServiceCUSS\Build\jar\*.* %2\%TIMESTAMP%\jar\*.*
+copy %LOGFILE% %2\%TIMESTAMP%\%LOGFILE%
+if exist m:\%1\SelfServiceCUSS\Build\Setup\Output copy m:\%1\SelfServiceCUSS\Build\Setup\Output\*.* %2\%TIMESTAMP%\*.*
