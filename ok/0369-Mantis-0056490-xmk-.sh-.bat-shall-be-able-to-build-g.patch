From 70a798dd46370cb7f90ca8a9707b49b1f45d32c9 Mon Sep 17 00:00:00 2001
From: mbrinkma <mbrinkma@2eb42a87-3827-0410-b767-dc15055982e4>
Date: Fri, 27 Mar 2020 15:45:12 +0000
Subject: [PATCH 369/462] Mantis 0056490: xmk[.sh|.bat] shall be able to build
 gradle-components of PLF

git-svn-id: svn://localhost/SelfServiceBuild/trunk@675 2eb42a87-3827-0410-b767-dc15055982e4
---
 SelfServiceBuild/Build/cpp/makefile.mak       |  36 +++++++++++-------
 .../Build/scripts/gradle-build-all.bat        |   8 ++--
 .../scripts/gradle/wrapper/gradle-wrapper.jar | Bin 53556 -> 54417 bytes
 .../gradle/wrapper/gradle-wrapper.properties  |  11 +++---
 SelfServiceBuild/Build/scripts/gradlew        |  26 ++++++++-----
 SelfServiceBuild/Build/scripts/gradlew.bat    |  14 ++-----
 .../Build/scripts/java8/gradlew.bat           |   2 +-
 7 files changed, 55 insertions(+), 42 deletions(-)

diff --git a/SelfServiceBuild/Build/cpp/makefile.mak b/SelfServiceBuild/Build/cpp/makefile.mak
index 11b671c..288bc5a 100644
--- a/SelfServiceBuild/Build/cpp/makefile.mak
+++ b/SelfServiceBuild/Build/cpp/makefile.mak
@@ -38,23 +38,23 @@ TRACE_DIRECTORIES = \
  Massai\cpp\Trace
 !endif
 
-!if "$(VC_COMPILER_VC6)" == "6" 
-ICA_DIRECTORIES = 
+!if "$(VC_COMPILER_VC6)" == "6"
+ICA_DIRECTORIES =
 !else
 ICA_DIRECTORIES = \
   Massai\cpp\Installer_CustomAction
 !endif
 
-!if "$(VC_COMPILER_VC6)" == "6" 
-INSTALLER_DIRECTORIES = 
+!if "$(VC_COMPILER_VC6)" == "6"
+INSTALLER_DIRECTORIES =
 !else
 INSTALLER_DIRECTORIES = \
   Build\installer \
   Massai\cpp\LogService\services\NagiOS
 !endif
 
-!if "$(VC_COMPILER_VC6)" == "6" 
-APPLICATION_DIRECTORIES = 
+!if "$(VC_COMPILER_VC6)" == "6"
+APPLICATION_DIRECTORIES =
 !else
 APPLICATION_DIRECTORIES =
 !endif
@@ -150,14 +150,14 @@ PLATFORM_DIRECTORIES= \
 
 PLATFORM_INSTALLER_DIRECTORIES= \
  $(INSTALLER_DIRECTORIES)
- 
+
 PLATFORM_MVN_DIRECTORIES= \
  $(APPLICATION_DIRECTORIES) \
  ..\SelfServiceCommon\Massai\java\barcodeParserLib \
  ..\SelfServicePlatform\Massai\java\AdminApplication \
  ..\SelfServicePlatform\Massai\java\ExtractBilling \
  ..\SelfServicePlatform\Massai\java\KioskStatusReporting
- 
+
 ABLS_DIRECTORIES= \
  Massai\idl\Massai \
  Services\cpp\msABLSController
@@ -216,7 +216,7 @@ common-lint:
 
 platform-lint:
     for %%i in ($(PLATFORM_DIRECTORIES)) do cd "$(PLATFORMROOT)\%%i" && $(MAKE) lint
-    
+
 abls-lint:
     for %%i in ($(ABLS_DIRECTORIES)) do cd "$(ABLSROOT)\%%i" && $(MAKE) lint
 
@@ -323,20 +323,30 @@ javadoc:
 # Maven
 ######################################################################
 
-maven: 
+maven:
     - for %%i in ($(PLATFORM_MVN_DIRECTORIES)) do cd "$(PLATFORMROOT)\%%i" && $(MAVEN) install
 
 maven-clean:
     - for %%i in ($(PLATFORM_MVN_DIRECTORIES)) do cd "$(PLATFORMROOT)\%%i" && $(MAVEN) clean
-    
+
+######################################################################
+# gradle
+######################################################################
+
+gradle:
+    - cd "$(BUILDROOT)\Build\scripts" && gradle-build-all build
+
+gradle-clean:
+    - cd "$(BUILDROOT)\Build\scripts" && gradle-build-all clean
+
 ######################################################################
 # Installer
 ######################################################################
 
-installer: 
+installer:
     - for %%i in ($(PLATFORM_INSTALLER_DIRECTORIES)) do cd "$(PLATFORMROOT)\%%i" && $(MAKECMD)
 
 installer-clean:
     - for %%i in ($(PLATFORM_INSTALLER_DIRECTORIES)) do cd "$(PLATFORMROOT)\%%i" && $(MAKECMD) clean
-    
+
 
diff --git a/SelfServiceBuild/Build/scripts/gradle-build-all.bat b/SelfServiceBuild/Build/scripts/gradle-build-all.bat
index 20dcf53..5b9f6e6 100644
--- a/SelfServiceBuild/Build/scripts/gradle-build-all.bat
+++ b/SelfServiceBuild/Build/scripts/gradle-build-all.bat
@@ -16,9 +16,11 @@ rem 2017-03-17 mbrinkma
 rem - creation
 rem
 
-set BASE_DIR=..\..\..\SelfServicePlatform\Massai\java
+set BASE_DIR=%PLATFORMROOT%\Massai\java
 
-set BUILD_ARGS=clean build
+set BUILD_ARGS=%1 %2 %3
+
+if "%BUILD_ARGS%" == "" set BUILD_ARGS=clean build
 
 call :callGradle %BASE_DIR%\DeviceSimulation\illumination-device-sim %BUILD_ARGS%
 rem call :callGradle %BASE_DIR%\DeviceSimulation\device-reg-test %BUILD_ARGS%
@@ -35,7 +37,7 @@ goto :eof
     shift
     set RESULT_FILE=%1
     shift
-  :no_copy_result  
+  :no_copy_result
   echo building %TOP_DIR%
   if exist %TOP_DIR%\settings.gradle (
     call gradlew --no-daemon -c %TOP_DIR%\settings.gradle -x test %1 %2 %3 %4
